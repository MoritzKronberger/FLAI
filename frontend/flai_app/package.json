{
  "name": "flai_app",
  "version": "0.0.1",
  "contributors": [
    "Anja Krendlinger, Eva Kunzmann, Kieu Pham, Lea Jell, Martin Kohnle, Moritz Kronberger, Stefanie Jany"
  ],
  "description": "Vue.js Frontend Application using Typescript and Vite",
  "keywords": [
    "Vue",
    "Typescript",
    "Vite"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "jest src",
    "lint": "eslint \"src/**/*.{js,vue,ts,tsx}\"",
    "lint-fix": "eslint \"src/**/*.{js,vue,ts,tsx}\" --fix",
    "prettier-fix": "prettier --write \"src/**/*.{js,vue,ts,tsx}\""
  },
  "dependencies": {
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/test-utils": "^2.0.0-rc.16",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.4",
    "vite": "^2.6.4",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "eslint \"src/**/*.{js,vue,ts,tsx}\" --fix",
      "eslint \"src/**/*.{js,vue,ts,tsx}\""
    ]
  }
}
